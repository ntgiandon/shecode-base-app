{
  "name": "appbase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "setup-env-dev": "cp -rf .env.development ./ios/EnvConfig.xcconfig && ENV=development ./envscript.sh && cp -rf .env.development .env",
    "setup-env-stage": "cp -rf .env.staging ./ios/EnvConfig.xcconfig && ENV=staging ./envscript.sh && cp -rf .env.staging .env",
    "setup-env-prod": "cp -rf .env.production ./ios/EnvConfig.xcconfig && ENV=production ./envscript.sh && cp -rf .env.production .env",
    "run-main-jsbundle": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle",
    "android:dev": "yarn setup-env-dev && react-native run-android --variant=developmentdebug --appIdSuffix=dev",
    "android:stage": "yarn setup-env-stage && react-native run-android --variant=stagingdebug --appIdSuffix=stage",
    "android:prod": "yarn setup-env-prod && react-native run-android --variant=productiondebug",
    "ios:dev": "yarn setup-env-dev && yarn run-main-jsbundle && react-native run-ios --scheme \"appbase-dev\"  --configuration DebugDev --udid='00008020-00180C3C0130003A'",
    "ios:stage": "yarn setup-env-stage && yarn run-main-jsbundle && react-native run-ios --scheme \"appbase-stage\"  --configuration DebugStage --udid='00008020-00180C3C0130003A'",
    "ios:prod": "yarn setup-env-prod && yarn run-main-jsbundle && react-native run-ios --scheme \"appbase\"  --configuration Debug --udid='00008020-00180C3C0130003A'",
    "codepush:android:dev": "appcenter codepush release-react -a linhh.phv/app-base-android -d Development",
    "codepush:android:stage": "appcenter codepush release-react -a linhh.phv/app-base-android -d Staging",
    "codepush:android:prod": "appcenter codepush release-react -a linhh.phv/app-base-android -d Production",
    "codepush:android:mandatory:dev": "appcenter codepush release-react -a linhh.phv/app-base-android -d Development -m",
    "codepush:android:mandatory:stage": "appcenter codepush release-react -a linhh.phv/app-base-android -d Staging -m",
    "codepush:android:mandatory:prod": "appcenter codepush release-react -a linhh.phv/app-base-android -d Production -m",
    "codepush:ios:dev": "appcenter codepush release-react -a linhh.phv/app-base -d Development",
    "codepush:ios:stage": "appcenter codepush release-react -a linhh.phv/app-base -d Staging",
    "codepush:ios:prod": "appcenter codepush release-react -a linhh.phv/app-base -d Production",
    "codepush:ios:mandatory:dev": "appcenter codepush release-react -a linhh.phv/app-base -d Development -m",
    "codepush:ios:mandatory:stage": "appcenter codepush release-react -a linhh.phv/app-base -d Staging -m",
    "codepush:ios:mandatory:prod": "appcenter codepush release-react -a linhh.phv/app-base -d Production -m",
    "codepush:dev": "yarn codepush:android:dev && yarn codepush:ios:dev",
    "codepush:stage": "yarn codepush:android:stage && yarn codepush:ios:stage",
    "codepush:prod": "yarn codepush:android:prod && yarn codepush:ios:prod",
    "codepush:mandatory:dev": "yarn codepush:android:mandatory:dev && yarn codepush:ios:mandatory:dev",
    "codepush:mandatory:stage": "yarn codepush:android:mandatory:stage && yarn codepush:ios:mandatory:stage",
    "codepush:mandatory:prod": "yarn codepush:android:mandatory:prod && yarn codepush:ios:mandatory:prod",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@notifee/react-native": "^7.7.1",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/netinfo": "^9.3.9",
    "@react-native-firebase/app": "^17.4.2",
    "@react-native-firebase/messaging": "^17.4.2",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.3.6",
    "patch-package": "^6.5.1",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "react-native-code-push": "^8.0.1",
    "react-native-config": "^1.5.0",
    "react-native-maps": "^1.7.1",
    "react-native-maps-directions": "^1.9.0",
    "react-native-safe-area-context": "^4.5.1",
    "react-native-screens": "^3.20.0",
    "react-redux": "^8.0.5",
    "reactotron-react-native": "^5.0.3",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
